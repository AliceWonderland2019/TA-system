{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "class-methods-use-this": [
      1
    ],
    "indent": [
      1,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "linebreak-style": [
      0
    ],
    "max-len": [
      1,
      120
    ],
    "no-unused-vars": [
      1
    ]
  }
}
