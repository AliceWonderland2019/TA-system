    "type": "git",
    "url": "git+https://github.com/rama41222/node-logger.git"
  },
  "keywords": [
    "node",
    "logger",
    "'node-logger'"
  ],
  "author": "rama41222",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rama41222/node-logger/issues"
  },
